function game_over()
	score=0
	cls(5)
	map(-4,43)
	camera(0,64)
	print("you win!",15,85,0)
	print("it took you "..finish_time.." seconds")
	print("thank you for playing!",24,175,0)
	
	print("press ❎ to play again")
	if btn(❎)then
			score=0
			finish_time=0
			for zanahoria in all (zanahorias) do
				del(zanahorias,zanahoria)
			end
			
			newgame=true
			scene="menu"
		end
end
