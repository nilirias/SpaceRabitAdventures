-- el codigo carry gracias profe

function collision_checks()
	local size = 8
 for i=1,#zanahorias do
  local zan=zanahorias[i]
  if p.x+size<zan.x      or --
			  p.x     >zan.x+size or --
     p.y+size<zan.y      or --
			  p.y     >zan.y+size then --
			-- no collision
		else
			return i
		end
 end
 return 0
end

